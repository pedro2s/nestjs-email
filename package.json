{
	"name": "@pedro2s/nestjs-email",
	"version": "0.1.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"release": "npm publish",
		"clean": "rimraf dist && rimraf coverage",
