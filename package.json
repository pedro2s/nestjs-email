		"prebuild": "npm run clean && npm run check-engines",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"NestJS",
		"Email",
		"Mailer",
		"SendGrid",
		"MailGun",
		"SMTP"
	],
	"author": "Pedro Santana",
	"license": "MIT",
	"type": "commonjs",
	"description": "Email Providers for NestJS",
	"engines": {
		"node": ">=20.x",
		"npm": ">=10.8.x"
	},
	"devEngines": {
		"node": ">=20.x",
		"npm": ">=10.8.x"
	},
	"files": [
		"package.json",
		"dist",
		"README.md"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pedro2s/nestjs-email.git"
	},
	"bugs": {
		"url": "https://github.com/pedro2s/nestjs-email/issues"
	},
	"homepage": "https://github.com/pedro2s/nestjs-email#readme",
	"devDependencies": {
		"@nestjs/common": "^10.4.11",
		"@nestjs/core": "^10.4.11",
		"@sendgrid/mail": "^8.1.4",
		"@types/node": "^22.10.0",
		"fbjs-scripts": "^3.0.1",
		"liquidjs": "^10.20.1",
		"mailersend": "^2.3.0",
		"mailgun.js": "^11.1.0",
		"rimraf": "^6.0.1",
		"typescript": "^5.7.2"
	}
